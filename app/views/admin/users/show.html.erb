<h1>User Information</h1>
<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <%= form_for :user do |f| %>
    Name : <%= f.text_field :name, class: "form-control", :disabled => true%><br>
    Birthday : <%= f.text_field :birthday, class: "form-control", :disabled => true %><br>
    Address : <%= f.text_area :address, class: "form-control", :disabled => true %><br>
    Email : <%= f.text_area :email, class: "form-control", :disabled => true %><br>
    <%= link_to "Back to list user", admin_users_path, class: "form-control btn btn-success" %>
    <% end %>
  </div>
</div>

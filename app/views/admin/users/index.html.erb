<h1 align="center">Users List</h1>
<div class="row col-lg-8 col-lg-offset-2">
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Id</th>
        <th>User name</th>
        <th>Birth day</th>
        <th>Address</th>
        <th>Email</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.id %></td>
          <td><%= link_to user.name, admin_user_path(user) %></td>
          <td><%= user.birthday %></td>
          <td><%= user.address %></td>
          <td><%= user.email %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

<h1>New category</h1>
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@category) do |f| %>
    	<%= render 'shared/error_messages' %>
      <%= f.label :name %>
      <%= f.text_field :name, class: "form-control" %>
      <br>
      <%= f.submit "Create new Category", class: "btn btn-primary" %>
    <% end %>
  </div>
